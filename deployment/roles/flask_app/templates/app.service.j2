[Unit]
Description=Gunicorn instance to serve myFlaskApp
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/myFlaskApp/
Environment="PATH=/home/ubuntu/myFlaskApp/env/bin"
ExecStart=/home/ubuntu/myFlaskApp/env/bin/gunicorn --workers 3 --bind unix:app.sock -m 007 wsgi:app

[Install]
WantedBy=multi-user.target